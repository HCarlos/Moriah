{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.11.1",
        "fideloper/proxy": "^4.4.0",
        "fruitcake/laravel-cors": "^2.0.2",
        "intervention/image": "^2.5.1",
        "laravel/framework": "7.28",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "php-parallel-lint/php-console-color": "^0.3.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "phpoffice/phpspreadsheet": "^1.14.1",
        "picqer/php-barcode-generator": "^0.2.2",
        "predis/predis": "^1.1.4",
        "setasign/fpdf": "^1.8.2",
        "spatie/laravel-permission": "^3.16",
        "styde/html": "^1.9",
        "yajra/laravel-datatables-oracle": "^9.11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan cache:clear"
        ],
        "post-install-cmd": [
            "@php artisan cache:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
