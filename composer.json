{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "^2.13.2",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.4",
        "intervention/image": "^2.6.1",
        "laravel/framework": "^7.30.4",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^2.5",
        "php-parallel-lint/php-console-color": "^0.3.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "phpoffice/phpspreadsheet": "^1.18.0",
        "picqer/php-barcode-generator": "^0.2.2",
        "predis/predis": "^1.1.7",
        "setasign/fpdf": "^1.8.3",
        "spatie/laravel-permission": "^3.18.0",
        "styde/html": "^1.9.1",
        "yajra/laravel-datatables-oracle": "^9.18.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.19"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan config:cache",
            "@php artisan cache:clear",
            "@php artisan config:clear"
        ],
        "post-install-cmd": [
            "@php artisan config:cache",
            "@php artisan cache:clear",
            "@php artisan config:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
